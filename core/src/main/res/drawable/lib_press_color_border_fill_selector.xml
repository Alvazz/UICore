<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/lib_bg_press" />
            <corners android:radius="@dimen/lib_radius_normal" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/lib_bg_press" />
            <corners android:radius="@dimen/lib_radius_normal" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/lib_line" android:color="@color/colorPrimaryDark" />
            <corners android:radius="@dimen/lib_radius_normal" />
        </shape>
    </item>
</selector>